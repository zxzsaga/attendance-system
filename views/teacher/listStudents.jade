extend layout

append body
  .container
    .row
      .col-sm-3
        .panel.panel-default
          .panel-heading
            h3.panel-title 查询
          .panel-body
            a(href='/teacher') 请假查询
            hr
            a(href='/teacher/dailyAttendanceIndex') 签到查询
      .col-sm-9
        .panel.panel-default
          .panel-heading
            h3.panel-title 学生列表
          .panel-body
            if (students)
              each name in students
                p #{ name }
